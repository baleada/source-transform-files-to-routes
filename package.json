{
  "name": "@baleada/source-transform-files-to-routes",
  "version": "0.4.6",
  "description": "Transform function, compatible with Baleada Source Transform, to replicate Nuxt/Next/Sapper functionality of generating routes based on file and directory structure",
  "type": "module",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    "require": "./lib/index.cjs",
    "import": "./lib/index.js"
  },
  "scripts": {
    "prepare": "tsc && rollup --config rollup.config.ts",
    "test:only": "run() { uvu -r ts-node/register tests/$2 $1.test.ts$; }; run",
    "test": "npm run test:only ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baleada/source-transform-files-to-routes.git"
  },
  "keywords": [
    "baleada",
    "files",
    "file-based routing",
    "pages",
    "routes",
    "vue router",
    "react router"
  ],
  "author": {
    "name": "Alex Vipond",
    "email": "hello@alexvipond.dev",
    "url": "https://alexvipond.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baleada/source-transform-files-to-routes/issues"
  },
  "homepage": "https://baleada.netlify.com",
  "devDependencies": {
    "@baleada/prepare": "^0.5.0",
    "rollup": "^2.45.2",
    "tailwindcss": "^2.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "uvu": "^0.5.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@rollup/pluginutils": "^4.1.0",
    "nanoid": "^3.1.10",
    "nanoid-dictionary": "^3.0.0"
  }
}
